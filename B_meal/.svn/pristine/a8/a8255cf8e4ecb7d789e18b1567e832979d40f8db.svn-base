                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="14dp"
                            android:text="이태원/한남"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:textColor="#b3b3b3"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/textIntro"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="7dp"
                            android:text="TextView"
                            android:textColor="#b3b3b3"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_horizontal"
                        android:gravity="center_horizontal"
                        android:orientation="vertical" >

                        <ImageView
                            android:id="@+id/imageView"
                            android:layout_width="31dp"
                            android:layout_height="31dp"
                            android:layout_marginTop="47dp"
                            android:src="@drawable/view" />

                        <TextView
                            android:id="@+id/textView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="2dp"
                            android:layout_marginTop="4dp"
                            android:text="조회"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:textColor="#ffffff"
                            android:textSize="9sp" />

                        <TextView
                            android:id="@+id/storeVisitCnt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="2"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#ffffff"
                            android:textSize="9sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:orientation="vertical" >

                        <ImageView
                            android:id="@+id/checkMayplace"
                            android:layout_width="31dp"
                            android:layout_height="31dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginRight="15dp"
                            android:layout_marginTop="47dp"
                            android:background="@drawable/b_meal_not_tap" />

                        <TextView
                            android:id="@+id/TextBmeal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="2dp"
                            android:layout_marginTop="4dp"
                            android:text="비밀장소"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:textColor="#ffffff"
                            android:textSize="9sp" />

                        <TextView
                            android:id="@+id/storeMyPlaceCnt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="2"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:textColor="#ffffff"
                            android:textSize="9sp" />
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>

        </FrameLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >
        </LinearLayout>

        <TextView
            android:id="@+id/textDietor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="6dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="19dp"
            android:text="에디터의 한마디"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#393939"
            android:textSize="15sp" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:background="#66c448"
            android:scaleType="fitXY"
            android:src="@drawable/greenline" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <com.tacademy.b_meal.login.MLRoundedImageView
                android:id="@+id/imageEditorProfile"
                android:layout_width="41dp"
                android:layout_height="41dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="21dp"
                android:src="@drawable/face" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/editorName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="23dp"
                    android:text="허태영"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="#393939"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/editorSiteBlog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:text="www.blog.naver.com/gloria"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="#1088dc"
                    android:textSize="12sp" />
            </LinearLayout>
        </LinearLayout>

        <TextView
            android:id="@+id/editorTalk"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="21dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="41dp"
            android:layout_marginTop="21dp"
            android:text="100% 호밀빵 위주의 메뉴로 먹으면서도 죄책감을 덜 수 있는 곳. 자자한 명성대로 맛있는 빵은 다이어터가 아닌 친구들도 충분히 즐길 수 있는 곳. 그리고 ½ 포장 된 제품의 경우 다이어트 용 한끼로 적당하다."
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="#393939"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="360dp"
            android:layout_height="14dp"
            android:background="#f2f2f2"
            android:text="@null" />

        <FrameLayout
            android:id="@+id/FrameLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="9dp"
            android:layout_marginTop="21dp" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <ImageView
                        android:id="@+id/imageView2"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/food_menu" />

                    <TextView
