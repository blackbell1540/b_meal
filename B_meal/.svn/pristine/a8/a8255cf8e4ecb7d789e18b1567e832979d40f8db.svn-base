                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#393939"
                        android:textSize="12sp" />

                </LinearLayout>

                <TextView
                    android:id="@+id/storeRecMenu"
                    android:layout_width="207dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="74dp"
                    android:layout_marginTop="8dp"
                    android:text="일이삼사오륙칠파구십일이삼자오륙칠팔"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="#898989"
                    android:textSize="12sp" />

            </LinearLayout>

        </FrameLayout>

        <FrameLayout
            android:id="@+id/FrameLayout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="9dp"
            android:layout_marginRight="41dp"
            android:layout_marginTop="14dp" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <ImageView
                        android:id="@+id/imageFoodMenu2"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        android:src="@drawable/food_menu1" />

                    <TextView
                        android:id="@+id/storeMenu2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="40dp"
                        android:text="대표 메뉴"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#393939"
                        android:textSize="12sp" />

                </LinearLayout>

                <TextView
                    android:id="@+id/storePriMenu"
                    android:layout_width="207dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="74dp"
                    android:layout_marginTop="8dp"
                    android:text="무화과 호밀빵 3000원 일이삼사오륙칠팔구십"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="#898989"
                    android:textSize="12sp" />

            </LinearLayout>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/FrameLayout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginBottom="9dp"
            android:layout_marginRight="41dp"
            android:layout_marginTop="14dp" >

            <ImageView
                android:id="@+id/imagePhone"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="15dp"
                android:src="@drawable/food_phone" />

            <TextView
                android:id="@+id/storePhone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="74dp"
                android:text="02-123-1234"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#1088dc"
                android:textSize="12sp" />

        </FrameLayout>

        <FrameLayout
            android:id="@+id/FrameLayout5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="9dp"
            android:layout_marginRight="41dp"
            android:layout_marginTop="14dp" >

            <ImageView
                android:id="@+id/imageRuntime"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="15dp"
                android:src="@drawable/food_runtime" />

            <TextView
                android:id="@+id/storeRunTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="74dp"
                android:text="10:00~23:00"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#393939"
                android:textSize="12sp" />

        </FrameLayout>

        <FrameLayout
            android:id="@+id/FrameLayout6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="9dp"
            android:layout_marginRight="41dp"
            android:layout_marginTop="14dp" >

            <ImageView
                android:id="@+id/imagePrice"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="15dp"
                android:src="@drawable/food_price" />

            <TextView
                android:id="@+id/storePrice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="74dp"
                android:text="예산 : 4000원~10000원"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#393939"
                android:textSize="12sp" />

        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="21dp"
            android:layout_marginTop="14dp" >

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="15dp"
                android:src="@drawable/icon_takeout" />

            <TextView
                android:id="@+id/storeTakeOut"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="74dp"
                android:text="포장가능"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="#393939"
                android:textSize="12sp" />

        </FrameLayout>

        <FrameLayout
            android:layout_width="360dp"
            android:layout_height="72dp"
            android:layout_gravity="center" >

            <com.skp.Tmap.TMapView
                android:id="@+id/mapRest"
                android:layout_width="360dp"
                android:layout_height="72dp"
                android:clickable="false"
                android:focusable="true"
                android:longClickable="false" >
            </com.skp.Tmap.TMapView>

            <TextView
                android:id="@+id/buttonAddr"
                android:layout_width="360dp"
                android:layout_height="72dp"
                android:background="#2667c449"
                android:clickable="true"
                android:paddingLeft="15dp"
                android:paddingTop="28dp"
                android:text="TextView"
                android:textColor="#393939"
                android:textSize="15sp" />

        </FrameLayout>

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="360dp"
            android:layout_height="7dp"
            android:background="#f2f2f2"
            android:text="@null" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="35dp" >

            <TextView
                android:id="@+id/textBmeal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="12dp"
                android:text="비밀 나누기"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#393939"
                android:textSize="15sp" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:id="@+id/textBmealCnt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="4dp"
                    android:text="7"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#b3b3b3"
                    android:textSize="10sp" />

            </FrameLayout>
        </LinearLayout>

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:background="#66c448"
            android:scaleType="fitXY"
            android:src="@drawable/greenline" />

        <LinearLayout
            android:id="@+id/layoutReview1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="34dp" >

            <com.tacademy.b_meal.login.MLRoundedImageView
                android:id="@+id/imageProfile1"
                android:layout_width="41dp"
                android:layout_height="41dp"
                android:layout_marginRight="16dp"
                android:src="@drawable/face" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" >

                    <TextView
                        android:id="@+id/textNickname1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#393939"
                        android:textSize="15sp" />
                </LinearLayout>

                <TextView
                    android:id="@+id/textReview1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textColor="#b3b3b3"
                    android:textSize="12sp" />

            </LinearLayout>

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="TextView"
                android:visibility="invisible" />

            <TextView
                android:id="@+id/textDate1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#b3b3b3"
                android:textSize="10sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/layoutReview2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="34dp" >

            <com.tacademy.b_meal.login.MLRoundedImageView
                android:id="@+id/imageProfile2"
                android:layout_width="41dp"
                android:layout_height="41dp"
                android:layout_marginBottom="19dp"
